    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/d3-array": "^2.0.0",
    "@types/d3-axis": "^1.0.12",
    "@types/d3-format": "^1.3.1",
    "@types/d3-scale": "^2.2.0",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-time": "^1.0.10",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.3",
