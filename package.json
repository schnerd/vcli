    "prettier": "^2.0.5",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/schnerd/vcli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "vcli",
    "macos": {
      "identifier": "com.vcli.vcli"
    }
  },
  "repository": "schnerd/vcli",
  "scripts": {
    "lint": "eslint src --ext ts,tsx",
    "posttest": "npm run lint",
    "prepack": "rm -rf lib && tsc -b && rm -rf src/client/.next && npm run client-build && mv src/client/.next lib/.next",
    "test": "jest",
    "test-watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "version": "oclif-dev readme && git add README.md",
    "client-build": "next build src/client",
  	"docs:build": "next build docs",
    "docs:dev": "next dev docs"
  },
  "types": "lib/index.d.ts"
}
